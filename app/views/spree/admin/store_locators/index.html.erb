<%= form_for @store_locator, url: admin_store_locators_path do |f| %>
  <fieldset>
    <legend align="center">New Store</legend>
    <%= render partial: 'form', locals: { f: f } %>
    <%= f.submit %>
  </fieldset>
<% end %>
<%= paginate @store_locators %>
<% if @store_locators.present? %>
  <%= render partial: 'store_locators' %>
<% else %>
  <div class="no-objects-found">No Stores found!</div>
<% end %>
<%= paginate @store_locators %>
